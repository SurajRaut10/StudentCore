@{
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Address</th>
            <th scope="col">Email</th>
            <th scope="col">Mob No</th>
            <th scope="col">Gender</th>
            <th scope="col">Course</th>
        </tr>
    </thead>
    <tbody>
        <tr>
                <th scope="row">@Model.Sid</th>
                <td>@Model.Sname</td>
                <td>@Model.Address</td>
                <td>@Model.Semail</td>
                <td>@Model.Smobno</td>
                <td>@Model.Gender</td>
                <td>@Model.Course</td>

            </tr>
        
    </tbody>
</table>